10 REM TIMEJUMP
20 W=40
30 PRINT "ENTER YEAR TO FO BACK TO:";
40 INPUT D
50 PRINT "ENTER PLACE";
60 INPUT P$
70 PRINT "ENTER THIS YEAR";
80 INPUT Y
90 PRINT "ENTER SPEED FACTOR";
100 INPUT S
110 PRINT
120 PRINT "ENTER JUMP-SPAN FACTOR"
130 PRINT "FROM MACHINE CONTROL:";
